{
  "name": "tipako",
  "version": "3.9.2",
  "description": "A picker with support for group or individual selections.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "node_modules/.bin/eslint src/*.jsx",
    "dev": "NODE_ENV='development' webpack --progress --colors --watch",
    "prod": "NODE_ENV='production' webpack --progress --colors -p",
    "sandbox": "NODE_ENV='development' webpack-dev-server --progress --colors --hot --inline ./sandbox/sandbox.jsx --config webpack.dev.config.js  --content-base ./../../sandbox"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:procore/particles.git/"
  },
  "author": "Procore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/procore/particles/issues"
  },
  "homepage": "https://github.com/procore/particles/tree/master/pickers/tipako",
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "recompose": "^0.24.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.21.0",
    "jest": "^20.0.4",
    "node-sass": "^3.8.0",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
